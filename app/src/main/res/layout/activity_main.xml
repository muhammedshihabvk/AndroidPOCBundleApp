<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="30dp"
    tools:context=".MainActivity">

   <Button
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:text="list View POC"
       android:id="@+id/listViewNavButton"/>

   <Button
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:text="Time Table List Generator"
       android:id="@+id/TimeTableListViewButton"/>

   <Button
   android:layout_width="match_parent"
   android:layout_height="wrap_content"
   android:text="Timer POC"
   android:id="@+id/timerButton"/>

   <Button
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:text="Timer App"
       android:id="@+id/eggTimerButton"/>



   <Button
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:text="Brain Trainer App"
       android:id="@+id/brainTrainerButton"/>


</LinearLayout>